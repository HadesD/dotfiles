set -g default-terminal "screen-256color"

# Plugins {

run-shell $HOME/.tmux/plugins/tmux-resurrect/resurrect.tmux
run-shell $HOME/.tmux/plugins/tmux-pain-control/pain_control.tmux
run-shell $HOME/.tmux/plugins/tmux-sensible/sensible.tmux

# }

# Key {

set -g prefix2 C-b                       # GNU-Screen compatible prefix
bind C-b send-prefix -2
unbind-key C-a

# }

set-option -s escape-time 10


